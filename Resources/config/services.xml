<container xmlns="http://symfony.com/schema/dic/services"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://symfony.com/schema/dic/services
                               http://symfony.com/schema/dic/services/services-1.0.xsd">

    <services>
        <service id="agallou.grunt_hash_assets.twig.assets" class="Agallou\GruntHashAssetsBundle\Twig\Assets">
            <argument>%agallou.grunt_hash_assets.parameters.assets_dir%</argument>
            <argument>%agallou.grunt_hash_assets.parameters.assets_base_path%</argument>
            <tag name="twig.extension" />
        </service>
    </services>
</container>
